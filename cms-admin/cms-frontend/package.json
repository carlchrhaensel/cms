{
  "name": "cms-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@tinymce/tinymce-react": "^3.14.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-notifications": "^1.7.3",
    "react-notifications-component": "^3.4.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "react-sortablejs": "^6.1.1",
    "sortablejs": "^1.15.0"
  },
  "scripts": {
    "start:dev": "react-scripts start -env=development",
    "start:local": "react-scripts start -env=local",
